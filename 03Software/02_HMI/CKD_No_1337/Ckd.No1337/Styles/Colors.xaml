<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:Ckd.No1337">

    <Color x:Key="BackgroundLight">#efefef</Color>
    <SolidColorBrush x:Key="BackgroundLightBrush" Color="{StaticResource BackgroundLight}" />

    <Color x:Key="BackgroundVeryLight">#fafafa</Color>
    <SolidColorBrush x:Key="BackgroundVeryLightBrush" Color="{StaticResource BackgroundVeryLight}" />

    <Color x:Key="ForgroundMain">#686868</Color>
    <SolidColorBrush x:Key="ForgroundMainBrush" Color="{StaticResource ForgroundMain}" />

    <Color x:Key="ForgroundVeryDark">#000000</Color>
    <SolidColorBrush x:Key="ForgroundVeryDarkBrush" Color="{StaticResource ForgroundVeryDark}" />

    <Color x:Key="ForgroundLight">#ffffff</Color>
    <SolidColorBrush x:Key="ForgroundLightBrush" Color="{StaticResource ForgroundLight}" />


</ResourceDictionary>